{
    "name": "leaflet-mapmatch",
    "description": "A plugin to align geometries to streetmap data",
    "main": "index.js",
    "scripts": {
        "test": "zuul --local -- test/test.js",
        "test-remote": "zuul -- test/test.js",
        "test-headless": "zuul --phantom test/test.js",
        "build": "browserify -s omnivore index.js > leaflet-mapmatch.js && uglifyjs leaflet-mapmatch.js -c -m > leaflet-mapmatch.min.js"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:mapbox/leaflet-mapmatch.git"
    },
    "browserify": {
        "transform": [
      "brfs",
      "hintify"
    ]
    },
    "keywords": [
    "leaflet",
    "mapmatch",
    "align",
    "geojson",
    "osm",
    "resample"
  ],
    "author": "Arun Ganesh",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/mapbox/leaflet-mapmatch/issues"
    },
    "homepage": "https://github.com/mapbox/leaflet-mapmatch",
    "devDependencies": {
        "browserify": "7.0.2",
        "tape": "2.4.2",
        "uglify-js": "2.4.16",
        "mapbox.js": "1.6.4"
    }
}