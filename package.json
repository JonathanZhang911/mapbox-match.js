{
  "name": "mapbox-mapmatch",
  "description": "A plugin to align geometries to streetmap data",
  "main": "index.js",
  "version": "0.1.0",
  "scripts": {
    "test": "zuul --local -- test/test.js",
    "build": "browserify -s mapmatch index.js > dist/mapbox.mapmatch.js && uglifyjs dist/mapbox.mapmatch.js -c -m > dist/mapbox.mapmatch.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mapbox/mapbox-mapmatch.git"
  },
  "browserify": {
    "transform": [
      "brfs",
      "hintify"
    ]
  },
  "keywords": [
    "leaflet",
    "mapmatch",
    "align",
    "geojson",
    "osm",
    "resample"
  ],
  "author": "Arun Ganesh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mapbox/mapbox-mapmatch/issues"
  },
  "homepage": "https://github.com/mapbox/mapbox-mapmatch",
  "devDependencies": {
    "browserify": "7.0.2",
    "tape": "2.4.2",
    "uglify-js": "2.4.16",
    "mapbox.js": "1.6.4"
  },
  "dependencies": {
    "brfs": "^1.4.0",
    "geojson-tidy": "^0.1.4",
    "hintify": "^0.1.0",
    "polyline": "0.0.3",
    "queue-async": "^1.0.7",
    "xhr": "^2.0.2",
    "zuul": "^3.0.0"
  }
}
